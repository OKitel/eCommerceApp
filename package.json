{
  "name": "ecommerceapp",
  "description": "Online eCommerce application to sell and buy products",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "check": "prettier --check .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest"
  },
  "dependencies": {
    "@commercetools/history-sdk": "^3.5.0",
    "@commercetools/importapi-sdk": "^4.6.2",
    "@commercetools/ml-sdk": "^3.4.1",
    "@commercetools/platform-sdk": "^5.0.0",
    "@commercetools/sdk-client-v2": "^2.2.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "vite-plugin-environment": "^1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-eslint": "^1.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OKitel/eCommerceApp.git"
  },
  "author": "Olga Kitel, Aleh Sakalou, Artem Lobovskii",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OKitel/eCommerceApp/issues"
  },
  "homepage": "https://github.com/OKitel/eCommerceApp#readme"
}
